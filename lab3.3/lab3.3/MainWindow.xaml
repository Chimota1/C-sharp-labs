<Window x:Class="lab3._3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab3._3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
    <StackPanel Margin="20">
        <TextBox x:Name="Text" Width="300" Height="50" Margin="0,0,0,0"></TextBox>
        <Button Width="200" Height="30" Margin="10" Content="Роздрукувати" Click="Button_Click"></Button>
        <TextBlock x:Name="Result" Width="300" Height="30"></TextBlock>
    </StackPanel>
    </Grid>
</Window>
