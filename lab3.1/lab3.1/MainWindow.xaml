<Window x:Class="lab3._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <TextBox x:Name="InputBox" Width="300" Height="30" Margin="0,0,0,10" />
            <Button Content="Обробити" Width="150" Height="30" Click="Process_Click" />
            <TextBlock x:Name="ResultBlock" FontSize="18" Margin="0,10,0,0" />
        </StackPanel>
    </Grid>
</Window>